        id        = "mob.app.dev.todo"
        version   = "1.0.0">

    <name>To Do</name>

    <description>
        A simple to do app
    </description>

    <author href="source.worcestercomputing.com" email="oani1_16@uni.worc.ac.uk">
        WBS Computing
